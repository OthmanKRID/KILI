@model Kili.Models.Vente.Catalogue

@{
    Layout = null;
    
}
<h3>Veuillez confirmer la suppression du catalogue</h3>

<div>
    <h4>Catalogue</h4>
    <hr />
    <dl class="dl-horizontal">
        
        <dt>
             @Html.HiddenFor(c => Model.CatalogueID)
        </dt>
        <dt>
            @Html.DisplayNameFor(c => Model.CatalogueName)
        </dt>

        <dd>
            @Html.DisplayFor(c => Model.CatalogueName)
        </dd>

        <dt>
            @Html.DisplayNameFor(c => Model.Description)
        </dt>

        <dd>
            @Html.DisplayFor(c => Model.Description)
        </dd>

    </dl>

     @using (Html.BeginForm()) {
         <div class="form-actions no-color">
            <input type="submit" value="Supprimer" class="btn btn-default" /> |
            @Html.ActionLink("Revenir aux catalogues", "IndexCatalogue")
        </div>
    }
</div>

