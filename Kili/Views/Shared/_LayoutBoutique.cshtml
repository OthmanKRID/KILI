@{
    
}

<!doctype html>
<html class="no-js" lang="en">
    <head>
        <!-- meta data -->
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
        <!--font-family-->
        <link href="https://fonts.googleapis.com/css?family=Roboto:100,100i,300,300i,400,400i,500,500i,700,700i,900,900i" rel="stylesheet">

        <!-- title of site -->
        <title>Ma boutique</title>

        <!-- For favicon png -->
        <link rel="shortcut icon" type="image/icon" href="assets/logo/favicon.png" />

        <!--font-awesome.min.css-->
        <link rel="stylesheet" href="~/css-boutique/font-awesome.min.css">

        <!--linear icon css-->
        <link rel="stylesheet" href="~/css-boutique/linearicons.css">

        <!--animate.css-->
        <link rel="stylesheet" href="~/css-boutique/animate-boutique-user.css">

        <!--owl.carousel.css-->
        <link rel="stylesheet" href="~/css-boutique/owl-boutique-user.carousel.min.css">
        <link rel="stylesheet" href="~/css-boutique/owl-boutique-user.theme.default.min.css">

        <!--bootstrap.min.css-->
        <link rel="stylesheet" href="~/css-boutique/bootstrap-boutique-user.min.css">

        <!-- bootsnav -->
        <link rel="stylesheet" href="~/css-boutique/bootsnav-boutique-user.css">

        <!--style.css-->
        <link rel="stylesheet" href="~/css-boutique/style-boutique-user.css">

        <!--responsive.css-->
        <link rel="stylesheet" href="~/css-boutique/responsive-boutique-user.css">

        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
            <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

    </head>

    <body>
        <!--[if lte IE 9]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
        <![endif]-->
        <!--welcome-hero start -->
        <header id="home" class="welcome-hero">
			@RenderSection("extraheader", false)
            <!-- top-area Start -->
			<div class="top-area">
				<div class="header-area">
					<!-- Start Navigation -->
				    <nav class="navbar navbar-default bootsnav  navbar-sticky navbar-scrollspy"  data-minus-value-desktop="70" data-minus-value-mobile="55" data-speed="1000">

				        <!-- Start Top Search -->
				        <div class="top-search">
				            <div class="container">
				                <div class="input-group">
				                    <span class="input-group-addon"><i class="fa fa-search"></i></span>
				                    <input type="text" class="form-control" placeholder="Search">
				                    <span class="input-group-addon close-search"><i class="fa fa-times"></i></span>
				                </div>
				            </div>
				        </div>
				        <!-- End Top Search -->

				        <div class="container">            
				            <!-- Start Atribute Navigation -->
				            <div class="attr-nav">
				                <ul>
				                	<li class="search">
				                		<a href="#"><span class="lnr lnr-magnifier"></span></a>
				                	</li><!--/.search-->
				                	<li class="nav-setting">
				                		<a href="#"><span class="lnr lnr-cog"></span></a>
				                	</li><!--/.search-->
                                    @await Html.PartialAsync("~/Views/Shared/_PanierPartial.cshtml")
				                </ul>
				            </div><!--/.attr-nav-->
				            <!-- End Atribute Navigation -->

				            <!-- Start Header Navigation -->
				            <div class="navbar-header">
				                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-menu">
				                    <i class="fa fa-bars"></i>
				                </button>
				                <a class="navbar-brand" href="@Url.ActionLink("IndexBoutique", "Vente")">Kili</a>

				            </div><!--/.navbar-header-->
				            <!-- End Header Navigation -->

				            <!-- Collect the nav links, forms, and other content for toggling -->
				            <div class="collapse navbar-collapse menu-ui-design" id="navbar-menu">
				                <ul class="nav navbar-nav navbar-center" data-in="fadeInDown" data-out="fadeOutUp">
				                    <li class=" scroll active"><a href="#home">home</a></li>
									<li class="scroll"><a href="#feature">Nouveauté</a></li>
				                    <li class="scroll"><a href="#new-arrivals">Nos produits</a></li>
				                    
				                   
				                </ul><!--/.nav -->
				            </div><!-- /.navbar-collapse -->
				        </div><!--/.container-->
				    </nav><!--/nav-->
				    <!-- End Navigation -->
				</div><!--/.header-area-->
			    <div class="clearfix"></div>

			</div><!-- /.top-area-->
			<!-- top-area End -->

            <div class="clearfix"></div>

            </div><!-- /.top-area-->
            <!-- top-area End -->

        </header><!--/.welcome-hero-->
        <!--welcome-hero end -->
        <!--feature start -->
        @RenderBody()
        <!--feature end -->
        <!--footer start-->
        <!--footer end-->
        <!-- Include all js compiled plugins (below), or include individual files as needed -->

        <script src="~/js-boutique/jquery.js"></script>

        <!--modernizr.min.js-->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js"></script>

        <!--bootstrap.min.js-->
        <script src="~/js-boutique/bootstrap.min.js"></script>

        <!-- bootsnav js -->
        <script src="~/js-boutique/bootsnav.js"></script>

        <!--owl.carousel.js-->
        <script src="~/js-boutique/owl.carousel.min.js"></script>


        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>


        <!--Custom JS-->
        <script src="~/js-boutique/custom.js"></script>

    </body>
</html>